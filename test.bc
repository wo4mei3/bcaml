let rec total = function
| [] -> 0
| x::xl -> x + total xl

let rec fact n =
  if n <= 0 then 1 else n * fact (n - 1)

let rec fibo n =
  if n < 2 then n
  else fibo (n - 1) + fibo (n - 2)

let _ = fact 10

let _ = fibo 10

